{
	"firestore": {
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"functions": {
		"source": "functions",
		"runtime": "nodejs10"
	},
	"hosting": {
		"public": "public",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "**",
				"function": "nextServer"
			}
		]
	},
	"storage": {
		"rules": "storage.rules"
	}
}
